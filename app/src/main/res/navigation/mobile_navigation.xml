<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_fragment_main">

    <fragment
        android:id="@+id/nav_fragment_main"
        android:name="com.fetchreward.hiring.ui.list.HiringListFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_main" >

        <action
            android:id="@+id/action_nav_fragment_main_to_nav_fragment_setting"
            app:destination="@id/nav_fragment_setting" />
    </fragment>

    <fragment
        android:id="@+id/nav_fragment_setting"
        android:name="com.fetchreward.hiring.ui.setting.SettingsFragment"
        android:label="@string/setting"
        tools:layout="@layout/fragment_setting" />
</navigation>